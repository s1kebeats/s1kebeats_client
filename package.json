{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build": "nuxt build",
    "build:main-css": "sass ./assets/styles/main.scss ./assets/styles/main.css",
    "build:tailwind": "pnpm run build:main-css && pnpm exec tailwindcss -i ./assets/styles/main.css -o ./assets/styles/output.css",
    "build:sb": "storybook build",
    "dev": "conc \"pnpm run format:all .\" \"nuxt dev\"",
    "dev:tailwind": "pnpm run build:tailwind -- --watch",
    "dev:sb": "conc \"pnpm run format:all .\" \"storybook dev -p 6006\" \"pnpm run dev:tailwind\"",
    "format:mod": "pnpm exec lint-staged",
    "format:all": "prettier --write",
    "lint": "eslint --fix .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nuxt/devtools": "latest",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@pinia/testing": "^0.1.3",
    "@storybook/addon-essentials": "7.2.0-rc.0",
    "@storybook/addon-interactions": "7.2.0-rc.0",
    "@storybook/addon-links": "7.2.0-rc.0",
    "@storybook/blocks": "7.2.0-rc.0",
    "@storybook/testing-library": "0.2.0",
    "@storybook/vue3": "7.2.0-rc.0",
    "@storybook/vue3-vite": "7.2.0-rc.0",
    "@types/node": "^18.17.1",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "@vue/test-utils": "^2.4.1",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "happy-dom": "^10.5.2",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.6.5",
    "nuxt-icon": "^0.4.2",
    "prettier": "^3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.2.0-rc.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.11",
    "@s1kebeats/s1kebeats-ui": "^3.7.1",
    "pinia": "^2.1.6",
    "sass": "^1.64.1"
  }
}
